<?xml version="1.0"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee" 
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
   version="2.0">

<!--Habilita o conteiner do spring, desabilitando o conteiner do jsf-->
<application>
    <el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>     
    
</application>
<converter>  
    <display-name>UsuarioConverter</display-name>  
    <converter-id>UsuarioConverter</converter-id>  
    <converter-class>br.com.financeiro.helpers.UsuarioConverter</converter-class>   
</converter> 
	
</faces-config>